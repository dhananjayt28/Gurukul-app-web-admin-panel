                        <div class="col-sm-3 col-md-3">
                            <h5> Date</h5>
                            @Html.EditorFor(model => Model.Date, new { htmlAttributes = new { @id = "iasMainDate", @class = "form-control bod_rad_0 common-date-picker", @placeholder = "Date", @readonly = true, @data_date_format = "dd-mm-yyyy" } })
                            <label for="iasMainDate"><i class="fa fa-calendar date-icon" aria-hidden="true"></i></label>
                        </div>
